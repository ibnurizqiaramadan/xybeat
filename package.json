{
  "name": "xyrus10-bot",
  "version": "1.0.0",
  "description": "A Discord bot built with TypeScript and discord.js v14",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "watch": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts --fix",
    "deploy-commands": "ts-node -r tsconfig-paths/register src/deploy-commands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discord.js"
  ],
  "author": "xyrus10",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.6",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3",
    "play-dl": "^1.9.7"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.11.0"
  }
}
